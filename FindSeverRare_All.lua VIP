-- FindSeverRare_All.lua
-- Tá»± Ä‘á»™ng load cáº£ 2 script: FindSeverRare vÃ  FindSeverRare_Discord

local function safeLoad(url)
    local ok, code = pcall(function()
        return game:HttpGet(url)
    end)
    if ok and code then
        local f, err = loadstring(code)
        if f then
            local ok2, err2 = pcall(f)
            if not ok2 then warn("[Loader] Error running "..url, err2) end
        else
            warn("[Loader] loadstring error for "..url, err)
        end
    else
        warn("[Loader] Cannot fetch "..url, code)
    end
end

-- ðŸ”¹ Link script cá»§a báº¡n
safeLoad("https://raw.githubusercontent.com/TommNger/MyHub.lua/main/FindSeverRare.lua")
safeLoad("https://raw.githubusercontent.com/TommNger/MyHub.lua/main/FindSeverRare_Discord.lua")

print("[FindSeverRare_All] Loaded both scripts!")
